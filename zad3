#include <iostream>
#include <math.h>

using namespace std;

int main (){

    cout<<"Podaj liczbe calkowita, ktorej dzielniki chcesz uzyskac: ";

    int n;
    cin>>n;

    cout<<endl<<"Dzielniki podanej liczy: ";

    for(int i=1;i<=n;i++){
        if(n%i==0){
            cout<<i<<", ";
        }
    }

    cout<<endl;

return 0;
}
